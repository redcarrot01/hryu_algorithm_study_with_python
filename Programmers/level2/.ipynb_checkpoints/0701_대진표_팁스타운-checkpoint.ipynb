   "source": [
    "다름 라운드의 번호를 구해야 한다.  \n",
    "구하는 방법은 (번호 +1)//2를 하여, 두 몫이 같아질때 대결한다."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "b2e5a39e",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "3"
      ]
     },
     "execution_count": 2,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "def solution(n,a,b):\n",
    "    answer = 0\n",
    "    while a!= b:\n",
    "        a = (a+1)//2\n",
    "        b = (b+1)//2\n",
    "        answer += 1\n",
    "    return answer                                \n",
    "solution(8, 4, 7)"
   ]
  }
