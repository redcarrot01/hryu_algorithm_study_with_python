   ],
   "source": [
    "def solution(n):\n",
    "    answer = ''\n",
    "    for i in range(n):\n",
    "        if i % 2==0:\n",
    "            answer+= '수'\n",
    "        else:\n",
    "            answer += '박'\n",
    "    return answer\n",
    "solution(3)"
   ]
  },
