   "source": [
    "n = int(input())\n",
    "arr = list(map(int, input().split()))\n",
    "arr.reverse()\n",
    "dp = [1]*n\n",
    "for i in range(n):\n",
    "    for j in range(i):\n",
    "        if arr[i] > arr[j]:\n",
    "            dp[i] = max(dp[j]+1, dp[i])\n",
    "print(max(dp))"
   ]
  }
