    "n = int(input())\n",
    "arr = list(map(int, input().split()))\n",
    "dp= [0]*n\n",
    "dp[0] = arr[0]\n",
    "for i in range(1, n):\n",
    "    dp[i] = max(arr[i], dp[i-1]+arr[i])\n",
    "print(max(dp))"
