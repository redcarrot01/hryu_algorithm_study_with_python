    "dp = [1e9]*5001\n",
    "\n",
    "        \n",
    "dp[3]=1; dp[5]=1\n",
    "for i in range(6, n+1):\n",
    "    dp[i] = min(dp[i-3], dp[i-5]) + 1\n",
    "    \n",
    "if dp[n]>=1e9:\n",
    "    print(-1)\n",
    "else:\n",
    "    print(dp[n])"
