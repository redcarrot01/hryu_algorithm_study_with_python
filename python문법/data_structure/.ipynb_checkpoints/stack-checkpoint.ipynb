   "source": [
    "리스트 변수로 스택을 다루는 pop, push 구현"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "ad6043dd",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "9"
      ]
     },
     "execution_count": 1,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "stack = list()\n",
    "def push(data):\n",
    "    stack.append(data)\n",
    "def pop():\n",
    "    data = stack[-1]\n",
    "    del stack[-1]\n",
    "    return data\n",
    "for i in range(10):\n",
    "    push(i)\n",
    "pop()"
   ]
  }
