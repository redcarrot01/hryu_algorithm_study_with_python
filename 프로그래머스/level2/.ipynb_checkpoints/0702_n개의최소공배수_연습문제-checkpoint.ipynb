    "from math import gcd\n",
    "def lcm(a, b):\n",
    "    return a*b//gcd(a,b)\n",
    "    \n",
    "def solution(arr):\n",
    "    while True:\n",
    "        if len(arr)==1:\n",
    "            return arr[0]\n",
    "        a=arr.pop(0)\n",
    "        b=arr.pop(0)\n",
    "        arr.append(lcm(a,b))\n",
    "solution([2,6,8,14])"
   ]
