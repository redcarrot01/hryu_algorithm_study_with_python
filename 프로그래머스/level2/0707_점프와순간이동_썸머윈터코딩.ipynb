    "def solution(n):\n",
    "    answer = 0\n",
    "    while n>0:\n",
    "        if n % 2==0:\n",
    "            n= n//2\n",
    "        else:\n",
    "            n -= 1\n",
    "            answer += 1\n",
    "    return answer\n",
    "solution(6)"
   ]
